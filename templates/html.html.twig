<!DOCTYPE html>
<!--suppress SpellCheckingInspection, HtmlUnknownTarget -->
<html class="no-js" lang="De">
<head>
        <meta charset="utf-8">
        <title>{{ title }}</title>
        <meta name="description" content="{{ description }}">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <meta property="og:title" content="">
        <meta property="og:type" content="">
        <meta property="og:url" content="">
        <meta property="og:image" content="">

        <!-- generics -->
        <link rel="icon" href="{{ canoncial }}/favicon/favicon.ico" type="image/x-icon">
        <link rel="icon" href="{{ canoncial }}/favicon/favicon-32.png" sizes="32x32">
        <link rel="icon" href="{{ canoncial }}/favicon/favicon-57.png" sizes="57x57">
        <link rel="icon" href="{{ canoncial }}/favicon/favicon-76.png" sizes="76x76">
        <link rel="icon" href="{{ canoncial }}/favicon/favicon-96.png" sizes="96x96">
        <link rel="icon" href="{{ canoncial }}/favicon/favicon-128.png" sizes="128x128">
        <link rel="icon" href="{{ canoncial }}/favicon/favicon-192.png" sizes="192x192">
        <link rel="icon" href="{{ canoncial }}/favicon/favicon-228.png" sizes="228x228">

        <!-- Android -->
        <link rel="shortcut icon" sizes="196x196" href=“{{ canoncial }}/favicon/favicon-196.png">

        <!-- iOS -->
        <link rel="apple-touch-icon" href="{{ canoncial }}/favicon/favicon-120.png" sizes="120x120">
        <link rel="apple-touch-icon" href="{{ canoncial }}/favicon/favicon-152.png" sizes="152x152">
        <link rel="apple-touch-icon" href="{{ canoncial }}/favicon/favicon-180.png" sizes="180x180">

        <!-- Windows 8  IE 10-->
        <meta name="msapplication-TileColor" content="#FFFFFF">
        <meta name="msapplication-TileImage" content="{{ canoncial }}/favicon/favicon-144.png">

        <!— Windows 8.1 + IE11 and above —>
        <meta name="msapplication-config" content="{{ canoncial }}/favicon/browserconfig.xml" />

        <link rel="stylesheet" href="css/vendor/normalize.css">
        <link rel="stylesheet" href="css/vendor/bootstrap/bootstrap.min.css">
        <link rel="stylesheet" href="css/vendor/fontawesome/css/all.min.css">
        <link rel="stylesheet" href="css/vendor/main.css">
        <link rel="stylesheet" href="css/theme.css">

        <meta name="theme-color" content="#002752">

</head>
<body>
    {% block header %}
            <nav class="container-fluid navbar navbar-expand-lg navbar-light bg-light">
                    <a class="navbar-brand" href="{{ canoncial }}">MVC App</a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                            <ul class="navbar-nav mr-auto">
                                    <li class="nav-item active">
                                            <a class="nav-link" href="/">Homepage <span class="sr-only">(current)</span></a>
                                    </li>
                                    <li class="nav-item">
                                            <a class="nav-link" href="/aufgabe_a">Aufgabe 1</a>
                                    </li>
                                    <li class="nav-item dropdown">
                                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> Aufgabe 2</a>
                                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                                <a class="dropdown-item" href="/aufgabe_b">Teil A</a>
                                                <a class="dropdown-item" href="/aufgabe_c">Teil B</a>
                                            </div>
                                    </li>
                            </ul>
                            <form class="form-inline my-2 my-lg-0">
                                    <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
                                    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
                            </form>
                    </div>
            </nav>
    {% endblock %}
    {% block teaser %}{% endblock %}
    {% block content %}{% endblock %}
    {% block footer %}
            <footer class="container">
                    <div class="row">
                            <div class="col mt-5 mb-2">
                                    <p class="text-center"><i class="fas fa-tools"></i> Call Rudra. Handcrafted from Rudra Martin Grellmann {martin@grellmann.eu} with <i class="fas fa-heart"></i></p>
                            </div>
                    </div>
            </footer>
    {% endblock %}
    {% block html_body_js_bottom %}
        <script src="js/vendor/jquery/jquery-3.5.1.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
        <script src="js/vendor/modernizr-3.11.2.min.js"></script>
        <script src="js/vendor/bootstrap/bootstrap.min.js"></script>
        <script src="css/vendor/fontawesome/js/all.min.js"></script>
        <script src="js/plugins.js"></script>
        <script src="js/main.js"></script>


    {% endblock html_body_js_bottom %}
</body>
</html>